{
  "name": "@solid-x/material",
  "version": "0.1.0",
  "description": "Design tokens for materialize components",
  "author": "Raghavan Renganathan",
  "license": "MIT",
  "homepage": "https://github.com/solidxjs/solid-x#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidxjs/solid-x",
    "directory": "packages/material"
  },
  "bugs": {
    "url": "https://github.com/solidxjs/solid-x/issues"
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "solid": "./dist/server/index.js",
      "import": "./dist/esm/index.js",
      "browser": "./dist/esm/index.js",
      "node": "./dist/server/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": {
      "types": "./dist/types/*/index.d.ts",
      "solid": "./dist/server/*/index.js",
      "import": "./dist/esm/*/index.js",
      "browser": "./dist/esm/*/index.js",
      "node": "./dist/server/*/index.js",
      "require": "./dist/cjs/*/index.js"
    },
    "./*.css": "./dist/*.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:dom && yarn build:ssr",
    "build:dom": "vite build",
    "build:ssr": "vite build --config vite.ssr.config.ts",
    "build:dev": "storybook build",
    "clean": "rimraf built dist",
    "create:component": "plop component",
    "dev": "storybook dev -p 6006",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@solid-x/tokens": "workspace:^",
    "@solidjs/router": "^0.8.3",
    "@solidjs/testing-library": "^0.8.4",
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/blocks": "^7.1.0",
    "@storybook/manager-api": "^7.1.0",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/theming": "^7.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/testing-library__jest-dom": "^5.14.8",
    "@vanilla-extract/css": "^1.12.0",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/esbuild-plugin": "^2.2.2",
    "@vanilla-extract/recipes": "^0.4.0",
    "@vanilla-extract/rollup-plugin": "^1.2.2",
    "@vanilla-extract/vite-plugin": "^3.8.2",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "babel-preset-solid": "^1.7.7",
    "glob": "^10.3.3",
    "jsdom": "^22.1.0",
    "plop": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.0",
    "solid-js": "^1.7.8",
    "storybook": "^7.1.0",
    "storybook-solidjs": "^1.0.0-beta.2",
    "storybook-solidjs-vite": "^1.0.0-beta.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vite-plugin-dts": "^3.2.0",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "solid-js": "^1.7.0"
  }
}
