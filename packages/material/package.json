{
  "name": "@solid-x/material",
  "version": "0.1.0",
  "description": "Design tokens for materialize components",
  "homepage": "https://github.com/solid-ui/solid-x#readme",
  "bugs": {
    "url": "https://github.com/solid-ui/solid-x/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solid-ui/solid-x",
    "directory": "packages/material"
  },
  "license": "MIT",
  "author": "Raghavan Renganathan",
  "scripts": {
    "build": "yarn clean && vite build",
    "build:dev": "storybook build",
    "clean": "rimraf built dist",
    "dev": "storybook dev -p 6006",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest"
  },
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/index.d.ts",
        "./dist/types/*/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "packageManager": "yarn@3.6.1",
  "devDependencies": {
    "@solid-x/tokens": "workspace:^",
    "@solidjs/testing-library": "^0.8.4",
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/blocks": "^7.1.0",
    "@storybook/manager-api": "^7.1.0",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/theming": "^7.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/testing-library__jest-dom": "^5.14.8",
    "@vanilla-extract/css": "^1.12.0",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/recipes": "^0.4.0",
    "@vanilla-extract/rollup-plugin": "^1.2.2",
    "@vanilla-extract/vite-plugin": "^3.8.2",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "glob": "^10.3.3",
    "jsdom": "^22.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "solid-js": "^1.7.8",
    "storybook": "^7.1.0",
    "storybook-solidjs": "^1.0.0-beta.2",
    "storybook-solidjs-vite": "^1.0.0-beta.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.2.0",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "solid-js": "^1.7.0"
  }
}
