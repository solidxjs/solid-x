{
  "compilerOptions": {
    "baseUrl": "./",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "noPropertyAccessFromIndexSignature": false,
    "outDir": "dist",
    "paths": {
      "@solid-x/material": ["../../packages/material/dist/types"],
      "@solid-x/material/*": ["../../packages/material/dist/types/*"],
      "~/*": ["./src/*"]
    },
    "skipLibCheck": true,
    "types": ["solid-js", "vite/client"]
  },
  "extends": "../../tsconfig.json"
}
